        <attribute name="edited" value="QW5nZ2EgQnJpbGx5YW50IFAgUztOQVdUSEE7MjAyNC0xMC0wOTswNDo1NzoxNCBQTTs0OzM1Mjc="/>
    </attributes>
    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="hargabuku, total, i, hasilakhir, hargaterkecil" type="Real" array="False" size=""/>
            <assign variable="total" expression="0"/>
            <input variable="hargabuku"/>
            <assign variable="i" expression="hargabuku"/>
            <assign variable="hargaterkecil" expression="9999"/>
            <while expression="hargabuku != 0">
                <if expression="i&lt;hargabuku">
                    <then>
                        <assign variable="i" expression="i"/>
                    </then>
                    <else>
                        <assign variable="i" expression="hargabuku"/>
                    </else>
                </if>
                <assign variable="total" expression="total + hargabuku"/>
                <input variable="hargabuku"/>
            </while>
            <if expression="total&gt;=100">
                <then>
                    <assign variable="hasilakhir" expression="total - i"/>
                </then>
                <else>
                    <assign variable="hasilakhir" expression="hasilakhir"/>
                </else>
            </if>
            <output expression="ToFixed(hasilakhir,2)" newline="True"/>
        </body>
    </function>
</flowgorithm>
